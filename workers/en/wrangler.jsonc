{
  "name": "supertrend-en",  // نام Worker (بدون تغییر)
  "main": "worker.js",   // مسیر فایل JS
  "compatibility_date": "2025-10-11",  // کاما اضافه شد اینجا!

  // vars (placeholderها – واقعی‌ها رو در dashboard بگذار)
  "vars": {
    "TELEGRAM_TOKEN": "",
    "ADMIN_CHAT_ID": "",
    "RESEND_API_KEY": "",
    "ADMIN_PASSWORD": "adminpass2025",
    "TRONSCAN_API_KEY": ""
  },

  // bindings برای D1 و KV (اگر نیاز نداری، حذف کن)
  "d1_databases": [
    {
      "binding": "DB",
      "database_name": "your-db-name",  // نام واقعی DBت رو بگذار
      "database_id": "your-db-id"
    }
  ],
  "kv_namespaces": [
    {
      "binding": "KV_ORDERS",
      "id": "your-kv-id"
    }
  ]
}
